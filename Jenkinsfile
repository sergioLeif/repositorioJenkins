genericSlave{
        GIT_REPO_URL = 'https://github.com/sergioLeif/repositorioJenkins.git'
        DEPLOY_LIBS_ARTIFACTORY = 'NO'
        LIB_POM_PATH='KETR-LIBS'

        //Valor para decidir si se desea generar una relese desde una rama master release o hotfix
        //Posibles valores YES | NO
        DEPLOYMENT_TASK='NO'

        //Valor para decidir si se queire realizar el despliegue automático en el primer entorno del cirucito
        //Posibles valores YES | NO
        AUTO_DEPLOY='NO'

        //Circuito sobre el que se realizará la el despliegue
        DEPLOYMENT_PLAN='CIR_KETR_TEST'

        //Proyecto JIRA
        JIRA_PROJECT_ID='KETR'

        //UUAA del proyecto
        UUAA='ketr'

        //Tipo de pase "Correctivo, Evolutivo..."
        DEPLOY_TYPE='CORRECTIVO'

        //Version de la release
        JIRA_AFFECTED_VERSION='Propia'

        //Tag desde el cual se generó la última release
        LAST_TAG='v4'

        //Entorno previo en el que se quiere desplegar
        //Posibles valores DESARROLLO | INTEGRADO
        ENVIRONMENT='DESARROLLO'

        //Indica si incorpora información sensible o no
    //Posibles valores YES | NO
    UPDATE_SENSITIVE_INFORMATION='NO'

    //Versión de JAVA
    //Posibles valores JDK_1.6 | JDK_1.7 | JDK_1.8
    JAVA_VERSION='JDK_1.7'

    //Versión de MAVEN
    //Posibles valores 3.0.3 | 3.3.9 | 3.5.0
    MAVEN_VERSION='Maven 3.3.9'

    //NODO='slave'

        ARQUITECTURA
        LENGUAJE
}
